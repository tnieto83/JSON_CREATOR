#ABNF 1.0 ISO8859-1;


language en-us;


root $spell = $GARBAGE* ($natoSpelling | $SIL)* $GARBAGE* ;


$natoSpelling =
    alpha       {A} |
    bravo       {B} |
    charlie     {C} |
    delta       {D} |
    echo        {E} |
    foxtrot     {F} |
    golf        {G} |
    hotel       {H} |
    india       {I} |
    juliett     {J} |
    kilo        {K} |
    lima        {L} |
    mike        {M} |
    november    {N} |
    oscar       {O} |
    papa        {P} |
    quebec      {Q} |
    romeo       {R} |
    sierra      {S} |
    tango       {T} |
    uniform     {U} |
    victor      {V} |
    whiskey     {W} |
    x-ray       {X} |
    yankee      {Y} |
    zulu        {Z} ;

$SIL =  "[SIL]" {};
